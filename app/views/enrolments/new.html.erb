<% provide(:title, "Enrolment") %>
<h1>Enrolments#new</h1>
<p>Find me in app/views/enrolments/new.html.erb</p>

<%= form_for :enrolment, url: enrolments_path do |f| %>

  <%= f.label :student_id %>
  <%= f.text_field :student_id %>

  <%= f.label :subject_id %>
  <%= f.text_field :subject_id %>

  <%= f.submit "Enrol on Subject" %>
<% end %>
