<h1>StaticPages#calendar</h1>
<p>Find me in app/views/static_pages/calendar.html.erb</p>

<%= month_calendar events: Meeting.all do |date, meetings| %>
  <%= date.day %>
  <% meetings.each do |meeting| %>
    <%= link_to meeting.subject, meeting %>
    <br/>
  <% end %>
<% end %>


<%= month_calendar events: Actionitem.all do |date, items| %>
  <%= date.day %>
  <% items.each do |item| %>
    <%= link_to item.name, item %>
    <br/>
  <% end %>
<% end %>
